{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": { "type": "string" },
            "moduleId": { "type": "string" },
            "name": { "type": "string" },
            "description": { "type": "string" },
            "data": {
                "oneOf": [
                    {
                        "type": "object"
                    },
                    {
                        "type": "array"
                    },
                    {
                        "type": "string"
                    },
                    {
                        "type": "number"
                    },
                    {
                        "type": "boolean"
                    }
                ]
            }
        },
        "required": ["id", "moduleId", "name", "description"]
    },
    "default": []
}